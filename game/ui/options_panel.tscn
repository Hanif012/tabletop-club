resource_name = "container_background"
script/source = "extends Container

export(Color) var background_color := Color.white setget set_background_color
export(int) var background_margin := 0 setget set_background_margin

func _draw():
	var pos := Vector2(-background_margin, 0.0)
	var size := rect_size + Vector2(2.0 * background_margin, 0.0)
	draw_rect(Rect2(pos, size), background_color, true)


func set_background_color(new_color: Color) -> void:
	background_color = new_color
	update()


func set_background_margin(new_margin: int) -> void:
	if new_margin < 0:
		return
	
	background_margin = new_margin
	update()
"

